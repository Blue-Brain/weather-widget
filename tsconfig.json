{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "sourceMap": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node",
    "paths": {
      "@UI/*": ["src/components/generic/*"],
      "@styles/*": ["src/assets/styles/*"],
      "@components/*": ["src/components/*"],
      "@pages/*": ["src/pages/*"],
    }
  }
}